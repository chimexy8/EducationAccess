@using System.Globalization
@model PaginatedList<FlashMoney.Models.TransactionHistoryModel>
@{
    ViewData["Title"] = "Index";
    ViewData["Header"] = "History";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}



