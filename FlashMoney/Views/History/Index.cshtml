@if (Model != null && Model.Any())
{
    <section class="content history">
        <div class="container">
            <div class="card animated fadeInUp fast delay-1">
                <div class="heading">
                    <a class="btn btn-link clear-btn">
                        <i class="icon auto">
                            <svg width="16" height="10" viewBox="0 0 16 10" fill="none"
                                 xmlns="http://www.w3.org/2000/svg">
                                <path d="M2.1668 5.83128H13.8325V4.1677H2.1668V5.83128ZM0.498047 9.16878H12.1689V7.50003H0.498047V9.16878ZM3.83555 0.835449V2.49899H15.5012V0.835449"
                                      fill="#FF7070" />
                            </svg>
                        </i>
                        <span>Clear All</span>
                    </a>

                    <div class="dropdown">
                        <a class="btn btn-link dropdown-toggle" id="fundtransfer-menu" data-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">
                            <span>Sort by:</span>
                            <strong>Date Performed</strong>
                            <i class="icon auto arrow">
                                <svg width="10" height="6" viewBox="0 0 10 6" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path opacity="0.4"
                                          d="M1.78119 0.218262L5.22069 3.65778L8.6602 0.218262L9.7207 1.27876L5.22069 5.77878L0.72069 1.27876L1.78119 0.218262Z"
                                          fill="white" />
                                </svg>
                            </i>
                        </a>

                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="fundtransfer-menu">
                            <a class="dropdown-item" asp-action="Index" asp-route-sortOrder="@ViewData["AmountSortParm"]">Amount</a>
                            <a class="dropdown-item" asp-action="Index" asp-route-sortOrder="@ViewData["DateSortParm"]">Date Performed</a>
                        </div>
                    </div>
                </div>
                <div class="flex-table">
                    <div class="tr thead">
                        <div class="td x2_">*</div>
                        <div class="td x1">Amount</div>
                        <div class="td x2_">Source / Recipient</div>
                        <div class="td x2">Date Performed</div>
                        <div class="td x1">Status</div>
                    </div>
                    @foreach (var item in Model)
                    {
                        <div class="tr">
                            <div class="td x2_">
                                @if (item.Description == "Wallet Funded" ||item.Description == "Credit")
                                {
                                    <i class="icon auto credit">
                                        <svg width="19" height="16" viewBox="0 0 19 16" fill="none"
                                             xmlns="http://www.w3.org/2000/svg">
                                            <path d="M9.493 4.3782V1.66093C9.493 1.06352 8.84309 0.735903 8.37887 1.08279L1.04416 6.71005C0.672781 6.99912 0.672781 7.57726 1.04416 7.86633L8.37887 13.4936C8.84309 13.8405 9.493 13.5129 9.493 12.9155V10.1596C13.3553 10.5451 15.7693 12.029 17.2548 14.7462C17.589 15.3437 18.4989 15.016 18.4061 14.3415C17.6819 9.19606 15.3608 5.16833 9.493 4.3782Z"
                                                  fill="#6AC895" />
                                        </svg>
                                    </i>
                                    <p>@item.Description</p>
                                }
                                else
                                {
                                    <i class="icon auto debit">
                                        <svg width="19" height="16" viewBox="0 0 19 16" fill="none"
                                             xmlns="http://www.w3.org/2000/svg">
                                            <path d="M9.68473 4.3782V1.66093C9.68473 1.06352 10.3346 0.735903 10.7989 1.08279L18.1336 6.71005C18.505 6.99912 18.505 7.57726 18.1336 7.86633L10.7989 13.4936C10.3346 13.8405 9.68473 13.5129 9.68473 12.9155V10.1596C5.8224 10.5451 3.40845 12.029 1.92294 14.7462C1.5887 15.3437 0.67882 15.016 0.771664 14.3415C1.49585 9.19606 3.81696 5.16833 9.68473 4.3782Z"
                                                  fill="#FF7070" />
                                        </svg>

                                    </i>
                                    <p>@item.Description</p>
                                }

                            </div>
                            <div class="td x1">
                                @if (item.Description == "Wallet Funded"||item.Description == "Credit")
                                {
                                    <strong class="amount credit">+@item.Amount.ToString("c", new CultureInfo("ha-Latn-NG"))</strong>
                                }
                                else
                                {
                                    <strong class="amount debit">-@item.Amount.ToString("c", new CultureInfo("ha-Latn-NG"))</strong>
                                }

                            </div>
                            <div class="td x2_">
                                <div class="avater">
                                    @if (item.Description == "Transfer"||item.Description == "Credit"||item.Description == "Transfer Charge"||item.Description == "Fund Wallet Charge")
                                    {
                                        if (item.ReceipientPassport != null)
                                        {
                                            
                                            <img class="round" src="@item.ReceipientPassport" />
                                        }
                                        else
                                        {
                                            <img class="round" src="~/assets/img/png/avatar.png" />
                                        }

                                        <p>
                                            @if (!string.IsNullOrEmpty(item.Receipient))
                                            {
                                                <strong class="ellipsis">@item.Receipient</strong>
                                            }
                                            @if (item.Description == "Credit")
                                            {
                                                <span class="ellipsis">@item.Phone</span>
                                            }
                                            else
                                            {
                                                <span class="ellipsis">@item.DestinationPhone</span>
                                            }

                                        </p>
                                    }
                                    else if (item.Description == "Wallet Funded")
                                    {
                                        if (item.CardType == "Master")
                                        {
                                            <img class="round" src="~/assets/img/png/card-master.png" />
                                        }
                                        else if (item.CardType == "Visa")
                                        {
                                            <img class="round" src="~/assets/img/png/card-visa.png" />
                                        }
                                        else
                                        {
                                            <img class="round" src="~/assets/img/png/nocard.png" />
                                        }

                                        <p>

                                            <strong class="ellipsis">@item.CardNumber</strong>
                                            <span class="ellipsis">@item.Phone</span>
                                        </p>
                                    }

                                </div>
                            </div>
                            <div class="td x2">
                                <p>@item.Date.ToString("d MMM yyyy hh:mm tt")</p>
                            </div>
                            <div class="td x1">
                                <span class="status green">Completed</span>
                            </div>

                            <div class="action">
                                @if (item.Description == "Transfer")
                                {
                                    <!--<a class="btn btn-link" data-toggle="modal" data-target="#modal-reclaim-funds">
                                        <i class="icon auto">
                                            <svg width="13" height="15" viewBox="0 0 13 15" fill="none"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path d="M12.834 9.49992L7.83398 14.4999L6.65065 13.3166L9.64232 10.3333H0.333984V0.333252H2.00065V8.66658H9.64232L6.65065 5.68325L7.83398 4.49992L12.834 9.49992Z"
                                                      fill="#5D2684" />
                                            </svg>
                                        </i>
                                        <span>Reclaim Funds</span>
                                    </a>-->
                                }

                                @if (item.Description == "Transfer" || item.Description == "Wallet Funded")
                                {
                                    <a class="btn btn-link text-red repeat_history_btn" data-itemid="@item.Id">
                                        <i class="icon auto">
                                            <svg width="14" height="18" viewBox="0 0 14 18" fill="none"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path d="M7.00065 4.16659V0.833252L2.83398 4.99992L7.00065 9.16658V5.83325C8.32673 5.83325 9.5985 6.36004 10.5362 7.29772C11.4739 8.2354 12.0007 9.50717 12.0007 10.8333C12.0007 12.1593 11.4739 13.4311 10.5362 14.3688C9.5985 15.3065 8.32673 15.8333 7.00065 15.8333C5.67457 15.8333 4.4028 15.3065 3.46512 14.3688C2.52744 13.4311 2.00065 12.1593 2.00065 10.8333H0.333984C0.333984 12.6014 1.03636 14.2971 2.28661 15.5473C3.53685 16.7975 5.23254 17.4999 7.00065 17.4999C8.76876 17.4999 10.4645 16.7975 11.7147 15.5473C12.9649 14.2971 13.6673 12.6014 13.6673 10.8333C13.6673 9.06514 12.9649 7.36945 11.7147 6.11921C10.4645 4.86896 8.76876 4.16659 7.00065 4.16659Z"
                                                      fill="#FF7070" />
                                            </svg>
                                        </i>
                                        <span>Repeat</span>
                                    </a>
                                }

                            </div>
                        </div>
                    }

                </div>

                @{
                    var prevDisabled = !Model.HasPreviousPage ? "disabled" : "";
                    var nextDisabled = !Model.HasNextPage ? "disabled" : "";
                }

                <ul class="pagination">
                    <li class="page-item @prevDisabled">
                        <a class="page-link" asp-action="Index"
                           asp-route-sortOrder="@ViewData["CurrentSort"]"
                           asp-route-pageNumber="@(Model.PageIndex - 1)"
                           asp-route-currentFilter="@ViewData["CurrentFilter"]">
                            <i class="icon auto">
                                <svg width="6" height="9" viewBox="0 0 6 9" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path d="M5.56055 1.06049L2.12103 4.49999L5.56055 7.9395L4.50005 9L2.6226e-05 4.49999L4.50005 -1.33514e-05L5.56055 1.06049Z"
                                          fill="#2F1343" />
                                </svg>

                            </i>
                            <span>Previous</span>
                        </a>
                    </li>
                    @for (var i = 1; i <= Model.TotalPages; i++)
                    {
                        if (Model.PageIndex == i)
                        {
                            <li class="page-item active"><a class="page-link" asp-action="Index" asp-route-pageNumber="@(i)">@(i)</a></li>
                        }
                        else
                        {
                            <li class="page-item"><a class="page-link" asp-action="Index" asp-route-pageNumber="@(i)">@(i)</a></li>
                        }
                    }
                    <li class="page-item  @nextDisabled">
                        <a class="page-link" asp-action="Index"
                           asp-route-sortOrder="@ViewData["CurrentSort"]"
                           asp-route-pageNumber="@(Model.PageIndex + 1)"
                           asp-route-currentFilter="@ViewData["CurrentFilter"]">
                            <span>Next</span>
                            <i class="icon auto">
                                <svg width="6" height="9" viewBox="0 0 6 9" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path opacity="0.3"
                                          d="M0.00195312 7.93951L3.44147 4.50001L0.00195312 1.0605L1.06245 0L5.56247 4.50001L1.06245 9.00001L0.00195312 7.93951Z"
                                          fill="#2F1343" />
                                </svg>

                            </i>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </section>



}
else
{
    <section class="content history ">
        <div class="container">
            <div class="card patterned empty animated fadeInUp fast delay-1">
                <div class="info">
                    <i class="icon auto">
                        <svg width="55" height="55" viewBox="0 0 55 55" fill="none"
                             xmlns="http://www.w3.org/2000/svg">
                            <path d="M4.58398 13.75V18.3333H32.084V13.75H4.58398ZM4.58398 22.9167V27.5H25.209V22.9167H4.58398ZM32.4779 24.6533L29.2373 27.8939L35.7183 34.375L29.2373 40.8561L32.4779 44.0967L38.959 37.6156L45.44 44.0967L48.6807 40.8561L42.1996 34.375L48.6807 27.8939L45.44 24.6533L38.959 31.1344L32.4779 24.6533ZM4.58398 32.0833V36.6667H25.209V32.0833H4.58398Z"
                                  fill="#FBB700" />
                        </svg>
                    </i>
                    <p>You haven’t perform any transaction yet</p>
                    <p>Make a transaction to have it show here in your history</p>

                    @*<a class="btn btn-link" href="./history-list.html">
                            <span>Action Link</span>
                            <i class="icon auto">
                                <svg width="6" height="9" viewBox="0 0 6 9" fill="none"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path d="M0 7.93951L3.43952 4.50001L0 1.0605L1.0605 0L5.56052 4.50001L1.0605 9.00001L0 7.93951Z"
                                          fill="#5D2684" />
                                </svg>
                            </i>
                        </a>*@
                </div>
            </div>
        </div>
    </section>
}


@section Modals {
    <div class="modal fade" id="modal-auth-generate" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="card form-card">
                    <!-- Loading  -->
                    <div class="form-section loading active">
                        <div class="la-ball-pulse">
                            <div></div>
                            <div></div>
                            <div></div>
                        </div>
                    </div>
                    <!-- Success & Error -->
                    <div class="form-section alert sub success message">
                        <div class="head">
                            <h4>Setup Authentication / USSD PIN</h4>
                        </div>
                        <img src="~/assets/img/png/alert-success.png" class="success-img" />
                        <img src="~/assets/img/png/alert-error.png" class="error-img" />
                        <div class="invisible successchange">
                            <h3>Pin Setup Successful</h3>
                            <p>
                                You can now perform secured transaction less than <strong>₦20,000.</strong>
                            </p>
                        </div>
                        <div class="invisible errorchange">
                            <h3>Pin Setup Unsuccessful</h3>
                            <p class="respmessage">

                            </p>
                        </div>
                        <div class="invisible networkchange">
                            <h3>Pin Setup Unsuccessful</h3>
                            <p class="respmessage">Pin Setup could not be completed please try again</p>
                        </div>



                        <a class="btn btn-primary continue-alert-btn" data-dismiss="modal">Continue</a>
                        <a class="btn btn-primary try-pin-again hidden">Try Again</a>
                    </div>


                    <!-- Generated Auth PIN -->
                    <div class="form-section auth-pin">
                        <div class="head sub">
                            <a href="#" class="btn btn-link" data-dismiss="modal">
                                <i class="icon auto">
                                    <svg width="16" height="15" viewBox="0 0 16 15" fill="none"
                                         xmlns="http://www.w3.org/2000/svg">
                                        <path d="M16 6.61564V8.42502H3.98955L9.49477 13.4055L8.08057 14.6849L0.161133 7.52033L8.08057 0.355713L9.49477 1.63513L3.98955 6.61564H16Z"
                                              fill="#5D2684" />
                                    </svg>
                                </i>
                            </a>
                            <h4>Setup Authorization / USSD PIN</h4>
                        </div>
                        <form>
                            <p>
                                Setup an Authorization / USSD PIN to enable you perform transactions less than
                                <strong>₦20,000</strong> and for use with our USSD Channel <strong>*329#</strong>
                            </p>

                            <p>
                                <strong>Note</strong> that if you want to perform transactions higher than
                                <strong>₦20,000</strong>, you will be
                                required to also set up a <strong>Transaction PIN</strong>
                            </p>

                            <hr class="dashed" />

                            <label>Enter your preferred Authorization PIN</label>
                            <div class="input-pin x4">
                                <input id="p1" type="text" placeholder="*" autocomplete=“off”/>
                                <input id="p2" type="text" placeholder="*" autocomplete=“off”/>
                                <input id="p3" type="text" placeholder="*" autocomplete=“off”/>
                                <input id="p4" type="text" placeholder="*" autocomplete=“off”/>
                            </div>
                            <label>Confirm Authorization PIN</label>
                            <div class="input-pin x4">
                                <input id="p5" type="text" placeholder="*" autocomplete=“off”/>
