<section class="content overview">
    <div class="container">
        <div class="content-area">
            <div class="summary">
                <div class="row">
                    <div class="col-xs-12 col-sm-7">
                        <div class="balance">
                            <label>Wallet Balance</label>
                            <h1><em>₦</em>&nbsp;@Model.Balance</h1>
                            <p>
                                A/C No:
                                @if (!string.IsNullOrEmpty(Model.Phone))
                                {
                                    <strong>@Model.Phone.Substring(Model.Phone.Length - 10)</strong>
                                }
                                &nbsp;&nbsp; | &nbsp;&nbsp; Last funded:
                                @if (!string.IsNullOrEmpty(Model.LastFunded))
                                {
                                    <strong>@Model.LastFunded</strong>
                                }
                            </p>

                            @if (ViewBag.HasResetPin != null && ViewBag.HasResetPin)
                            {
                                <span id="hasresetpin" class="d-none">1</span>
                            }
                            else
                            {
                                <span id="hasresetpin" class="d-none">0</span>
                            }
                        </div>
                    </div>
